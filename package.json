{
  "name": "nestjs-project-structure",
  "version": "0.0.0",
  "description": "Node.js framework NestJS project structure",
  "main": "dist/app",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "entity:load": "ts-node bin/entity",
    "entity:sync": "typeorm-ts-node-commonjs -d bin/ormconfig.ts schema:sync",
    "build": "nest build",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/app",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:repl": "nest start --entryFile repl",
    "test": "jest --detectOpenHandles -i",
    "test:e2e": "jest --config ./test/jest.e2e.ts -i",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.3.2",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.2",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-express": "^9.3.2",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.2.0",
    "@nestjs/typeorm": "^9.0.1",
    "apollo-server-express": "^3.11.1",
    "axios": "^1.3.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.17",
    "helmet": "^6.0.1",
    "mysql2": "^3.1.0",
    "nanoid": "^3.3.4",
    "nestjs-pino": "^3.1.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.8.0",
    "pino-http": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/testing": "^9.3.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^29.4.0",
    "@types/node": "^16.18.12",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/prompts": "^2.4.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "jest": "^29.4.1",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.3",
    "prompts": "^2.4.2",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typeorm-model-generator": "^0.4.6-no-engines",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatsMiaow/nestjs-project-structure.git"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://github.com/CatsMiaow/nestjs-project-structure#readme",
  "author": "CatsMiaow",
  "license": "MIT"
}
